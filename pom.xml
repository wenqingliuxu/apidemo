  <modelVersion>4.0.0</modelVersion>

  <groupId>com.info</groupId>
  <artifactId>project_foundation</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>Interview Demo</name>
